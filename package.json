{
  "name": "terminal-wallet-cli",
  "version": "1.3.0",
  "description": "Privacy Enhanced Command Line Interface EVM Wallet",
  "type": "commonjs",
  "main": "./dist/main.js",
  "bin": "./dist/main.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "postinstall": "patch-package",
    "start": "node --max-old-space-size=8192 --no-warnings --experimental-specifier-resolution=node dist/main.js",
    "build": "rimraf dist && npm run lint && tsc",
    "st2": "node --experimental-specifier-resolution=node dist/main.js",
    "circular": "madge -c src/**/*",
    "preship": "npm run build",
    "ship": "node ship.mjs --clean",
    "lint": "eslint src",
    "installdeps": "npm install --legacy-peer-deps",
    "release": "release-it"
  },
  "keywords": [
    "defi",
    "crypto",
    "wallet",
    "privacy",
    "0x",
    "swap",
    "private swap",
    "0zk",
    "railgun"
  ],
  "author": "anon",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@graphql-tools/utils": "^9.2.1",
    "@release-it/conventional-changelog": "^7.0.0",
    "@types/level-js": "4.0.2",
    "@types/leveldown": "4.0.3",
    "@types/node": "20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "caxa": "~3.0.1",
    "chai": "^4.3.7",
    "esbuild": "~0.19.0",
    "eslint": "^8.46.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "fs-extra": "~11.1.1",
    "libp2p": "^0.42.2",
    "madge": "^6.1.0",
    "mkdirp": "~3.0.1",
    "no-op": "~1.0.3",
    "patch-package": "~8.0.0",
    "react-native": "0.72.3",
    "release-it": "^16.1.5",
    "rimraf": "~5.0.1",
    "ts-node": "10.9.1",
    "tslib": "^2.4.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@libp2p/interface": "^1.3.0",
    "@railgun-community/cookbook": "2.6.8",
    "@railgun-community/curve25519-scalarmult-rsjs": "0.2.4",
    "@railgun-community/poseidon-hash-rsjs": "1.0.1",
    "@railgun-community/shared-models": "7.3.1",
    "@railgun-community/waku-broadcaster-client-node": "7.1.1",
    "@railgun-community/wallet": "10.1.2",
    "bn": "1.0.5",
    "colors": "^1.4.0",
    "enquirer": "2.3.6",
    "ethers": "github:Railgun-Community/ethers.js#v6.7.10",
    "level-js": "6.1.0",
    "leveldown": "6.1.1",
    "snarkjs": "0.7.0"
  }
}
